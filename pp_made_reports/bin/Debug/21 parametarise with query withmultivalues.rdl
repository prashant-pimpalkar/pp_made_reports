    <DataSet Name="DataSet2_region_parametrise">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select distinct region ,
case when region ='asia' then 'asia region'
when region ='africa' then 'africa region'
when region ='arab' then 'arab region'
else'unknow region' end as regionlable from sale</CommandText>
      </Query>
